# only ac_max_sed_lines should be used.
: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='bullet'
PACKAGE_TARNAME='bullet'
PACKAGE_VERSION='2.62'
PACKAGE_STRING='bullet 2.62'
